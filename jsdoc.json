{
  "title": "Tau",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["./lib"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "\\.spec\\.js$"
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "package": "./package.json",
    "readme": "./README.md",
    "recurse": true,
    "template": "./node_modules/docdash"
  },
  "plugins": ["plugins/markdown", "./jsdoc.cjs"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}
